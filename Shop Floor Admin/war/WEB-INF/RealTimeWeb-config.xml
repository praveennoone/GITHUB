	<!--  Handler Mapping used by the RealTime Servlet for Secured URLs -->
	<bean id="webUrlMapping"
		class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="interceptors">
			<list>
				<ref bean="loginContextInterceptor" />
				<ref bean="ajaxInterceptor"/>
			</list>
		</property>
		<property name="mappings">
			<props>
				<prop key="**/home.htm">controllerHome</prop>
