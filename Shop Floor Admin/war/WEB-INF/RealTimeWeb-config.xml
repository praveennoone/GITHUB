	 			<prop key="**/activityRejected.htm">controllerActivityRejected</prop>
				<prop key="**/employeeDetails.htm">controllerEmployeeDetails</prop>
	 			<prop key="**/nonProductivity.htm">controllerNonProductivity</prop>
	 			<prop key="**/showActivity.htm">controllerShowActivity</prop>
	 			<prop key="**/modifyInputFlag.htm">controllerModifyInputFlag</prop>
				<prop key="**/exceptionalActivity.htm">controllerExceptionalActivity</prop>
				<prop key="**/logonLogoffDetails.htm">controllerLogonLogoffDetails</prop>
				<prop key="**/resetItemWeight.htm">controllerResetItemWeight</prop>
				<prop key="**/currentOrderDetails.htm">controllerCurrentOrderDetails</prop>
				<prop key="**/sequenceDetails.htm">controllerSequenceDetails</prop>
				<prop key="**/pickSequence.htm">controllerPickSequence</prop>
				<prop key="**/deptWCAssetDetails.htm">controllerDeptWCAssetReport</prop>
				<prop key="**/pendingOrderDetails.htm">controllerPendingOrderDetails</prop>
				<prop key="**/activityLog.htm">controllerActivityLog</prop>
				<prop key="**/activityLogSummary.htm">controllerActivityLogSummary</prop>
				<prop key="**/performanceAssetwise.htm">controllerPerformanceAssetwiseReport</prop>
				<prop key="**/wipStatus.htm">controllerWIPStatus</prop>
				<prop key="**/employeeOutput.htm">controllerEmployeeOutput</prop>
				<prop key="**/componentDetail.htm">controllerComponentDetail</prop>
				<prop key="**/updateItemWeight.htm">controllerUpdateItemWeight</prop>
				<prop key="**/correctionInterfaceModifyMO.htm">CorrectionIntModifyMOController</prop>		
				<prop key="**/pphCorrectionInterface.htm">controllerPPHCorrection</prop>		
				<prop key="**/correctionTrackingDetails.htm">controllerCorrectionTrackingDetails</prop>
